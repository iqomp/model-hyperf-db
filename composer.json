{
    "name": "iqomp/model-hyperf-db",
    "description": "Hyperf DB driver for iqomp/model",
    "type": "library",
    "license": "MIT",
    "version": "2.0.2",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/model": "^2.0",
        "hyperf/db-connection": "^2.1"
    },
    "extra": {
        "hyperf": {
            "config": "Iqomp\\ModelHyperfDb\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\ModelHyperfDb\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    }
}
