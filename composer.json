{
    "name": "iqomp/model-hyperf-db",
    "description": "Hyperf DB driver for iqomp/model",
    "type": "library",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Iqbal Fauzi",
            "email": "iqbalfawz@gmail.com"
        }
    ],
    "require": {
        "iqomp/model": "^1.0",
        "hyperf/db-connection": "^2.1"
    },
    "extra": {
        "iqomp/config": "iqomp/config/"
    },
    "autoload": {
        "psr-4": {
            "Iqomp\\ModelHyperfDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iqomp\\ModelHyperfDb\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/phpcs --standard=PSR12 src"
    }
}
